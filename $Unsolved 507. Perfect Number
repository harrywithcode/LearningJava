class Solution {
    public boolean checkPerfectNumber(int num) {
        int result = 1;
        int end = num;
        
        
        for (int i = 2; i < end; i++) {
            if (num % i ==0 && i < end) {
                result += i;
                end = num / i;
                result += end;
            }
        }
        if (result == num) {
            return true;
        }
        return false;
    }
}
