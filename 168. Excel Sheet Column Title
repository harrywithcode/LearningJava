public class Solution {
    public String convertToTitle(int n) {
        String res = "";
        while(n != 0) {
            char ch = (char)((n - 1) % 26 + 65);   //求最后一位
            n = (n - 1) / 26;   //让倒数第二位变成最后一位
            res = ch + res;
        }
        return res;
    }
}

/*
1 A
1+26 AA
1+26*2 BA
1+26*26 ZA
1+26+26*26 AAA
*/
