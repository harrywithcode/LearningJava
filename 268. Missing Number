class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        int start = 0;
        int end = nums.length;
        
        while (start < end) {
            int mid = start + (end - start) / 2;
            if (mid - nums[mid] != 0) {
                end = mid;
            } else {
                start = mid + 1;
            }
        }
        return start;
    }
}
